<h1>Create new Project</h1>
<%= print session %>
<%form_for :project, :url => {:action=>'create'} do |f|%>

  <%= f.hidden_field :created_by, { :value => session[:user_id] } %>
  
   <p><label for="project_title">Title:</label>
    <%= f.text_field :title %></p>
  
   <p><label for="project_description">Description:</label>
  <%= f.text_area  :description %></p>
  
     <p><label for="project_resources">Resources:</label>
  <%= f.text_field  :resources %></p>
  
     <p><label for="project_carbon_critical">Carbon critical:</label>
     	<%= f.check_box :carbon_critical %>
  

  
    <p>Suitable for:
    <select id='discipline_ids' name="discipline_ids[]" multiple="multiple">
        <%= options_for_select(@disciplines1, @project.disciplines.collect {|d| d.id }) %>
      </select>
  </p>
  


  
  <%= f.submit "Create" %>
<%end%>
  <p><%= link_to 'Back', {:action => 'index'} %></p>
